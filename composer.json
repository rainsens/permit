{
    "name": "rainsens/rbac",
    "description": "A Laravel RBAC authorization package based upon http path",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rainsen",
            "email": "yusen@rainsen.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Rainsens\\Rbac\\": "src"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rainsens\\Rbac\\Tests\\": "tests"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rainsens\\Rbac\\Providers\\RbacServiceProvider"
            ],
            "aliases": {
                "Rbac": "Rainsens\\Rbac\\Facades\\Rbac"
            }
        }
    }
}
